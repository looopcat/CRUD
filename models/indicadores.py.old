class Indicadores:
    def __init__(self, id=None, nombre="", valor_actual=0.0):
        self._id = id
        self._nombre = nombre
        self._valor_actual = valor_actual

    # Getters
    def get_id(self):
        return self._id

    def get_nombre(self):
        return self._nombre

    def get_valor_actual(self):
        return self._valor_actual

    # Setters
    def set_id(self, id):
        self._id = id

    def set_nombre(self, nombre):
        self._nombre = nombre

    def set_valor_actual(self, valor_actual):
        self._valor_actual = valor_actual

    def __str__(self) -> str:
        return self._nombre